    "filein = open('rosalind_iev.txt')\n",
    "num = filein.read().split()\n",
    "\n",
    "p_list = [1, 1, 1, 0.75, 0.5, 0] # refer code of IPRB (Mendel's first law)\n",
    "EV_list =[]\n",
    "\n",
    "for index, num_of_parents in enumerate(num):\n",
    "    EV_list.append(2*int(num_of_parents)*p_list[index])\n",
    "\n",
    "print (sum(EV_list))"
   ]
